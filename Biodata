import java.util.*;
import java.lang.*;
import java.io.*;
class Biodata
{
	public static void main (String[] args) throws java.lang.Exception
	{
		String name = "sridevi";
        String nickname = "devi";
        String city = "chennai";
        int age = 32;
        float height = 5.3f;
        boolean married = true;
        char sex = 'F';
        double population = 4828853;
        System.out.println("\n name: + \n city: \n age: + \n height: + \n married: + \n sex: + \n population:" );
	}
}
